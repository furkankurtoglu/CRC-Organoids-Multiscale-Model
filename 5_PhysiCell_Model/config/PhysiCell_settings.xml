<!--         Full DOMAIN -->

		<x_min>-512</x_min>
		<x_max>512</x_max>
		<y_min>-2880</y_min>
		<y_max>2880</y_max>
		<z_min>-2880</z_min>
		<z_max>2880</z_max>
		<dx>32</dx>
		<dy>32</dy>
		<dz>32</dz>
		<use_2D>false</use_2D>		
        
        
        
        
        
<!--         <x_min>-128</x_min>
		<x_max>128</x_max>
		<y_min>-1440</y_min>
		<y_max>1440</y_max>
		<z_min>-1440</z_min>
		<z_max>1440</z_max>
		<dx>32</dx>
		<dy>32</dy>
		<dz>32</dz>
		<use_2D>false</use_2D> -->
	</domain>
	
	<overall>
		<max_time units="min">4320</max_time> <!-- 1 days * 24 h * 60 min -->
		<time_units>min</time_units>
		<space_units>micron</space_units>
	
		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>10</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder> <!-- use . for root --> 

		<full_data>
			<interval units="min">360</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">360</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<options>
		<legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
		<virtual_wall_at_domain_edge>true</virtual_wall_at_domain_edge>		
	</options>	

	<microenvironment_setup>
		
		<variable name="glucose" units="mMolar" ID="0">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">30000.0</diffusion_coefficient> <!--https://www.ncbi.nlm.nih.gov/pubmed/7597991 -->
				<decay_rate units="1/min">0.0</decay_rate> 
			</physical_parameter_set>
			<initial_condition units="molar">17.5</initial_condition>
			<Dirichlet_boundary_condition units="molar" enabled="false">17.5</Dirichlet_boundary_condition>
                <boundary_value ID="ymax" enabled="true">17.5</boundary_value>
		</variable>		
        
		<variable name="glutamine" units="mMolar" ID="1">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">30000.0</diffusion_coefficient>
				<decay_rate units="1/min">0.0</decay_rate> 
			</physical_parameter_set>
			<initial_condition units="molar">5.5</initial_condition>
			<Dirichlet_boundary_condition units="mMolar" enabled="false">5.5</Dirichlet_boundary_condition>
                <boundary_value ID="ymax" enabled="true">5.5</boundary_value>
		</variable>	        
		
		<variable name="lactate" units="mMolar" ID="2">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">30000.0</diffusion_coefficient>
				<decay_rate units="1/min">0.0</decay_rate> 
			</physical_parameter_set>
			<initial_condition units="molar">0.0</initial_condition>
			<Dirichlet_boundary_condition units="molar" enabled="false">0.0</Dirichlet_boundary_condition>
                <boundary_value ID="ymax" enabled="false">0.0</boundary_value>
		</variable>
		
		<options>
			<calculate_gradients>false</calculate_gradients>
			<track_internalized_substrates_in_each_agent>false</track_internalized_substrates_in_each_agent>
			<!-- not yet supported --> 
			<initial_condition type="matlab" enabled="false">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			<!-- not yet supported --> 
			<dirichlet_nodes type="matlab" enabled="false">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>
	
	<cell_definitions>
		<cell_definition name="CRC_WT" ID="0">
			<phenotype>
				<cycle code="5" name="live">  
					<!-- using higher than normal significant digits to match divisions in default code -->
					<phase_transition_rates units="1/min"> 
						<rate start_index="0" end_index="0" fixed_duration="false">0.0</rate>
					</phase_transition_rates>
				</cycle>	
				
				<death>  
					<model code="100" name="apoptosis"> 
						<death_rate units="1/min">0.0</death_rate>
						<!-- use phase_transition_rates OR phase_durations -->
						<!--
						<phase_transition_rates units="1/min">
							<rate start_index="0" end_index="1" fixed_duration="true">0.00193798</rate>
						</phase_transition_rates>
						-->
						<phase_durations units="min">
							<duration index="0" fixed_duration="true">0</duration>
						</phase_durations>
						<parameters>
							<unlysed_fluid_change_rate units="1/min">0.05</unlysed_fluid_change_rate>
							<lysed_fluid_change_rate units="1/min">0</lysed_fluid_change_rate>
							<cytoplasmic_biomass_change_rate units="1/min">1.66667e-02</cytoplasmic_biomass_change_rate>
							<nuclear_biomass_change_rate units="1/min">5.83333e-03</nuclear_biomass_change_rate>
							<calcification_rate units="1/min">0</calcification_rate>
							<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
						</parameters>
					</model> 

					<model code="101" name="necrosis">
						<death_rate units="1/min">0.0</death_rate>
						<!-- use phase_transition_rates OR phase_durations -->
						<!-- necrosis uses phase_duration[0] = 0 so that it always immediately
							 tries to transition and instead checks volume against the rupture
							 volume --> 
<!--						
						<phase_transition_rates units="1/min">
							<rate start_index="0" end_index="1" fixed_duration="false">9e9</rate>
							<rate start_index="1" end_index="2" fixed_duration="true">1.15741e-5</rate>
						</phase_transition_rates>
-->						
						<phase_durations units="min">
							<duration index="0" fixed_duration="true">0</duration>
							<duration index="1" fixed_duration="true">0</duration>
						</phase_durations>
						
						<parameters>
							<unlysed_fluid_change_rate units="1/min">0.05</unlysed_fluid_change_rate>
							<lysed_fluid_change_rate units="1/min">0</lysed_fluid_change_rate>
							<cytoplasmic_biomass_change_rate units="1/min">1.66667e-02</cytoplasmic_biomass_change_rate>
							<nuclear_biomass_change_rate units="1/min">5.83333e-03</nuclear_biomass_change_rate>
							<calcification_rate units="1/min">0</calcification_rate>
							<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
						</parameters>
					</model> 
				</death>					

				<volume>  
					<total units="micron^3">1000</total> <!-- 6.2 micron radius -->
					<fluid_fraction units="dimensionless">0.75</fluid_fraction>
					<nuclear units="micron^3">300</nuclear>
					
					<fluid_change_rate units="1/min">0.05</fluid_change_rate>
					<cytoplasmic_biomass_change_rate units="1/min">0.0045</cytoplasmic_biomass_change_rate>
					<nuclear_biomass_change_rate units="1/min">0.0055</nuclear_biomass_change_rate>
					
					<calcified_fraction units="dimensionless">0</calcified_fraction>
					<calcification_rate units="1/min">0</calcification_rate>
					
					<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
				</volume> 			
				
				<mechanics> 
					<cell_cell_adhesion_strength units="micron/min">0.4</cell_cell_adhesion_strength>
					<cell_cell_repulsion_strength units="micron/min">10.0</cell_cell_repulsion_strength>
					<relative_maximum_adhesion_distance units="dimensionless">1.25</relative_maximum_adhesion_distance>
					
					<options>
						<set_relative_equilibrium_distance enabled="false" units="dimensionless">1.8</set_relative_equilibrium_distance>
						<set_absolute_equilibrium_distance enabled="false" units="micron">15.12</set_absolute_equilibrium_distance>
					</options>
				</mechanics>
				
				<motility>  
					<speed units="micron/min">0</speed>
					<persistence_time units="min">1</persistence_time>
					<migration_bias units="dimensionless">0</migration_bias>
					
					<options>
						<enabled>false</enabled>
						<use_2D>true</use_2D>
						<chemotaxis>
							<enabled>false</enabled>
							<substrate>glucose</substrate>
							<direction>1</direction>
						</chemotaxis>
					</options>
				</motility>
				
				<secretion>
					<substrate name="glucose">
						<secretion_rate units="1/min">0.0</secretion_rate>
						<secretion_target units="substrate density">1</secretion_target>
						<uptake_rate units="1/min">0.0</uptake_rate> <!-- 0.0 -->
						<net_export_rate units="total substrate/min">0</net_export_rate> 
					</substrate> 

					<substrate name="glutamine">
						<secretion_rate units="1/min">0.0</secretion_rate>
						<secretion_target units="substrate density">1</secretion_target>
						<uptake_rate units="1/min">0.00005</uptake_rate>
						<net_export_rate units="total substrate/min">0</net_export_rate> 
					</substrate> 

					<substrate name="lactate">
						<secretion_rate units="1/min">0.0047</secretion_rate>
						<secretion_target units="substrate density">1</secretion_target>
						<uptake_rate units="1/min">0</uptake_rate>
						<net_export_rate units="total substrate/min">0</net_export_rate> 
					</substrate> 
				</secretion>
				<molecular />
			</phenotype>
			<custom_data>  
                <biomass_flux type="double" units="1/hr">0.0</biomass_flux>
                <k1_glucose type="double" units="1/min">0.00021238095</k1_glucose> <!-- k1_glucose = 0.223/60/17.5 = 0.00021238095 1/min, 0.223 = r*, 17.5 = rho*-->
				<k1_glutamine type="double" units="1/min">0.0000091</k1_glutamine> <!-- k1_glutamine = 0.003/60/5.5 = 0.0000091 1/min, 0.003 = r*, 5.5 = rho* -->
				<glucose_result type="double" units="1/min">0.0</glucose_result>  
				<glutamine_result type="double" units="1/hr">0.0</glutamine_result> 
				<int_g6p type="double" units="mM/hr">0</int_g6p>
                <int_fbp type="double" units="mM/hr">0</int_fbp>
				<int_g3P type="double" units="mM/hr">0</int_g3P>
				<int_pep type="double" units="mM/hr">0</int_pep>
				<int_lac type="double" units="mM/hr">0</int_lac>
				<int_gln type="double" units="mM/hr">0</int_gln>
				<int_glu type="double" units="mM/hr">0</int_glu>
			</custom_data>
		</cell_definition>
	</cell_definitions>
	
	<initial_conditions>
		<cell_positions type="csv" enabled="false">
			<folder>./config</folder>
			<filename>cells.csv</filename>
		</cell_positions>
	</initial_conditions>		
	
	<user_parameters>
		<!-- <random_seed type="int" units="dimensionless">0</random_seed>  -->
			
        
        <whole_well_simulation type="bool" units="dimensionless">true</whole_well_simulation>
        
        <!-- Intracellular Model Parameters -->
        <intracellular_simulation type="bool" units="dimensionless">true</intracellular_simulation>
        <DNN_biomass_normalizer type="double" units="dimensionless">100</DNN_biomass_normalizer>
 
 
            <!-- Intracellular Concentrations -->
        <min_g6p_conc type="double" description="" units="none">1.68</min_g6p_conc>
        <max_g6p_conc type="double" units="none">6</max_g6p_conc>        
        <min_fbp_conc type="double" units="none">0.16</min_fbp_conc>
        <max_fbp_conc type="double" units="none">1.08</max_fbp_conc> 
        <min_g3p_conc type="double" units="none">0.24</min_g3p_conc>
        <max_g3p_conc type="double" units="none">0.84</max_g3p_conc> 
        <min_pep_conc type="double" units="none">0.24</min_pep_conc>
        <max_pep_conc type="double" units="none">0.84</max_pep_conc> 
        <min_lac_conc type="double" units="none">6.4</min_lac_conc>
        <max_lac_conc type="double" units="none">19.2</max_lac_conc> 
        <min_gln_conc type="double" units="none">0.56</min_gln_conc>
        <max_gln_conc type="double" units="none">1.08</max_gln_conc> 
        <min_glu_conc type="double" units="none">0.88</min_glu_conc>
        <max_glu_conc type="double" units="none">1.56</max_glu_conc> 

        <!-- Seeding Method -->
        <two_dim_seeding type="bool" units="dimensionless">true</two_dim_seeding>
        
            <!-- 2D Seeding -->
        <minimum_number_of_monolayer_cells type="double" units="none">1083</minimum_number_of_monolayer_cells>
        <maximum_number_of_monolayer_cells type="double" units="none">1291</maximum_number_of_monolayer_cells>
        
            <!-- 3D Seeding -->
        <number_of_organoids type="int" units="none">180</number_of_organoids>
		<initial_tumor_radius type="double" units="none">50</initial_tumor_radius>
        
	</user_parameters>
	
</PhysiCell_settings>